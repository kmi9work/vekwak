$('div[id*="student_"][id*="login"]').each(function(){$(this).hide()});
$('div[id*="student_info_"]').each(function(){$(this).remove()});
$("<%= escape_javascript render(:file => 'students/info.html') %>").appendTo('#avatar_<%= @id %>_<%= @path %>');
$("#student_info_<%= @id %>_<%= @path %>").slideToggle("fast");